{
  "name": "git-graph-extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,vue}\"",
    "watch": "nodemon --watch src --ext js,ts,vue,jsx,tsx --exec \"npm run format && npm run build\""
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "@gitgraph/js": "^1.4.0",
    "@octokit/rest": "^21.1.1",
    "d3": "^7.9.0"
  }
}
